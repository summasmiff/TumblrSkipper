{
	"manifest_version": 2,
	"name": "TumblrSkipper",
	"version": "0.1",
	"description": "Skips the sidebar for you when you click a Tumblelog on your Dashboard",
	"browser_action": {
    "default_icon": "TS.png"
  },
	"content_scripts": [
		{
			"matches": ["https://www.tumblr.com/dashboard"],
			"js": ["content.js"]
		}
	],
	"permissions": ["tabs"]
}
